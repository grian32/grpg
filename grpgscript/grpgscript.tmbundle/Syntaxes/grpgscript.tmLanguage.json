{
  "name": "grpgscript",
  "scopeName": "source.grpgscript",
  "fileTypes": ["grpgscript"],
  "patterns": [
    {
      "name": "keyword.control.grpgscript",
      "match": "\\b(var|fnc|return|if|else)\\b"
    },
    { "name": "constant.language.boolean.true", "match": "\\b(true)\\b" },
    { "name": "constant.language.boolean.false", "match": "\\b(false)\\b" },
    { "name": "support.function.builtin", "match": "\\b(println|len|push|unshift|concat)\\b" },
    {
      "name": "string.quoted.double.grpgscript",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.grpgscript",
          "match": "\\\\."
        }
      ]
    },
    {
      "name": "constant.numeric.grpgscript",
      "match": "\\b\\d+\\b"
    },
    {
      "name": "variable.other.grpgscript",
      "match": "\\b[a-zA-Z_]+\\b"
    },
    {
      "name": "punctuation.definition.block.grpgscript",
      "match": "[{}()]"
    },
    {
      "name": "punctuation.separator.grpgscript",
      "match": ",|;"
    }
  ]
}
